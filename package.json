{
  "name": "ossedge",
  "version": "0.1.0",
  "description": "ðŸš€ Reverse proxy for OSS, a lightweight CDN alternative.",
  "type": "module",
  "exports": {
    ".": {
      "types": "./lib/types/index.d.ts",
      "default": "./lib/esm/index.js"
    }
  },
  "types": "./lib/types/index.d.js",
  "files": [
    "lib"
  ],
  "bin": {
    "ossedge": "./lib/esm/bin/start.js"
  },
  "scripts": {
    "dev": "listhen -w ./src/app.ts",
    "clean": "rimraf ./lib ./dist",
    "prebuild": "npm run clean",
    "build:lib": "tsc -p ./tsconfig.esm.json",
    "build:standalone": "ncc build ./src/bin/start.ts -o dist -t && cp ./.env ./.env.production ./dist",
    "build": "npm run build:lib && npm run build:standalone",
    "test": "ava",
    "prepare": "husky install",
    "release": "npm run build &&np"
  },
  "engines": {
    "node": ">= 18"
  },
  "repository": {
    "url": "imyelo/ossedge",
    "type": "git"
  },
  "author": "yelo <zhihuzeye@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/imyelo/ossedge/issues"
  },
  "homepage": "https://github.com/imyelo/ossedge#readme",
  "devDependencies": {
    "@types/supertest": "^6.0.2",
    "@vercel/ncc": "^0.38.1",
    "@yelo/eslint-config": "^0.2.0",
    "@yelo/prettier-config": "^0.1.0",
    "@yelo/tsconfig": "^0.3.0",
    "ava": "^5.2.0",
    "eslint": "^8.38.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "listhen": "^1.7.2",
    "np": "^7.7.0",
    "prettier": "^2.8.7",
    "prettier-eslint-cli": "^6.0.1",
    "rimraf": "^4.4.1",
    "supertest": "^7.0.0",
    "tsimp": "^2.0.11",
    "typescript": "~5.0.3"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--import=tsimp"
    ],
    "files": [
      "test/**/*",
      "!test/helpers/**/*"
    ]
  },
  "prettier": "@yelo/prettier-config",
  "lint-staged": {
    "*.{md,js,ts,json}": [
      "prettier-eslint --write"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "dependencies": {
    "ali-oss": "^6.21.0",
    "cache-manager": "^5.7.6",
    "cache-manager-fs-hash": "^2.0.0",
    "dotenv-flow": "^4.1.0",
    "h3": "^1.12.0",
    "pino": "^9.4.0"
  }
}
